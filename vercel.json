{
  "functions": {
    "api/webhook.ts": {
        "memory": 1024,
        "maxDuration": 31
    }
  },
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/webhook.ts"
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api", "permanent": false }]
}
