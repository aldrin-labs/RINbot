{
  "functions": {
    "api/webhook.ts": {
      "memory": 1024,
      "maxDuration": 55
    },
    "api/cronjob-update-pool-provider-caches": {
      "maxDuration": 45
    }
  },
  "crons": [
    {
      "path": "/api/cronjob-update-pool-provider-caches.ts",
      "schedule": "*/5 * * * *"
    }
  ],
  "version": 2,
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/webhook.ts"
    }
  ],
  "redirects": [{ "source": "/", "destination": "/api", "permanent": false }]
}
