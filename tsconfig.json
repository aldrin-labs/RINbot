{
  "compilerOptions": {
    "strict": true,
    "target": "es2022",
    "module": "ES2022",
    "lib": ["es2020"],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "public",
    "resolveJsonModule": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
